<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.InfoCorp" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="colorPrimary">@color/black</item>

        <item name="android:colorBackground">@color/black</item>
        <item name="android:navigationBarColor">@color/second_background_color</item>
    </style>

    <style name="Theme.InfoCorp" parent="Base.Theme.InfoCorp" />

    <style name="MyStyle.TextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="android:textColorHint">@color/hint_text_color</item>
        <item name="hintTextColor">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="cursorColor">@color/white</item>
        <item name="boxStrokeColor">@color/white</item>
        <item name="boxStrokeWidthFocused">1dp</item>
        <item name="boxStrokeWidth">1dp</item>
    </style>


    <style name="MyStyle.SearchView" parent="Widget.AppCompat.SearchView.ActionBar">
        <item name="android:editTextColor">#FFFFFF</item>
        <item name="android:textColorHint">@color/hint_text_color</item>
        <item name="hintTextColor">#FFFFFF</item>
        <item name="android:queryHint">ssssssss</item>
    </style>

    <style name="MyAppearance.SearchView" parent="Widget.AppCompat.SearchView.ActionBar">
        <item name="android:textSize">8sp</item>

    </style>
</resources>